generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Account {
  id        String   @id @default(uuid())
  email     String   @unique
  password  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Team {
  id              Int      @id @default(autoincrement())
  depthChartArray String?
  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt
}
